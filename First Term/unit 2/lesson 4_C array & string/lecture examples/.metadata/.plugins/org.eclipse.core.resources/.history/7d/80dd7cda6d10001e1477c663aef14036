#include <stdio.h>
int main()
{
	float x[]={5,16,22,3.5,15};
	for(int i=0; i<sizeof(x)/4; i++)
	{
	float y=5*x[i]*x[i]+3*x[i]+2;
	printf("y(%f) = %f\n",x,y);
	}
}
