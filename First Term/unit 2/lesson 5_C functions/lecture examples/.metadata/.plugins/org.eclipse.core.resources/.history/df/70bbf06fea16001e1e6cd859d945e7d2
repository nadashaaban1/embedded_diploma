#include <stdio.h>
#include <string.h>
int findName(char names[][14],int n,char *name)
{
	int flag=0;
		for(int i=0;i<n;i++)
		{
			flag=!stricmp(name,names[i][14]);
		}
		return flag;
}
int main()
{
	char name[14];
	char names[5][14]={"Alaa","Osama","Mamdouh","Samy","Hossain"};
	puts("Enter your first name: ");
	gets(name);
	if(findName(names,5,name)==1)
		puts("Welcome");
	else
		puts("Goodbye");

}
